{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "List function groups by ids failed event",
  "properties": {
    "base_event": {
      "$ref": "./base.json#/definitions/base_event"
    },
    "errorMessage": {
      "type": "string",
      "description": "Error message",
      "example": "Could not list function groups"
    },
    "ids": {
      "type": "string",
      "description": "Comma separated ids",
      "example": "1,2,3"
    }
  }
}