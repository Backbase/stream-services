{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "javaType": "com.backbase.pandp.accesscontrol.query.rest.spec.v2.accesscontrol.accessgroups.serviceagreements.ServiceAgreementBase",
  "description": "Service agreement item",
  "properties": {
    "id": {
      "$ref": "definitions.json#/definitions/uuid",
      "description": "Service agreement ID",
      "example": "0955e686-d31e-4216-b3dd-5d66161d536d"
    },
    "externalId": {
      "description": "External ID of Service Agreement",
      "$ref": "definitions.json#/definitions/externalId",
      "example": "0955e686-d31e-4216-b3dd-5d66161d536d"
    },
    "name": {
      "type": "string",
      "description": "Service agreement name",
      "minLength": 1,
      "example": "Service Agreement 1"
    },
    "description": {
      "type": "string",
      "description": "Service agreement description",
      "minLength": 1,
      "example": "Service Agreement between A and B"
    },
    "isMaster": {
      "type": "boolean",
      "default": false,
      "description": "Is master service agreement",
      "example": false
    },
    "validFrom": {
      "type": "string",
      "format": "date-time",
      "description": "Start date and time of the service agreement."
    },
    "validUntil": {
      "type": "string",
      "format": "date-time",
      "description": "End date and time of the service agreement."
    }
  },
  "required": [
    "id",
    "name",
    "description"
  ]
}