{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Context Service Agreement item",
  "properties": {
    "serviceAgreementName": {
      "type": "string",
      "description": "Service Agreement name"
    },
    "serviceAgreementId": {
      "$ref": "definitions.json#/definitions/uuid",
      "description": "Service Agreement id"
    },
    "serviceAgreementMaster": {
      "type": "boolean",
      "description": "Service Agreement Master"
    },
    "externalId": {
      "description": "External ID of Service Agreement",
      "type": "string",
      "$ref": "definitions.json#/definitions/externalId"
    },
    "description": {
      "type": "string",
      "description": "Brief text to describe service agreement",
      "minLength": 1,
      "maxLength": 255
    }
  },
  "required":[
    "serviceAgreementName",
    "serviceAgreementId",
    "serviceAgreementMaster"
  ]
}