{
  "request": {
    "method": "GET",
    "urlPathPattern": "/access-control/integration-api/v3/access-control/legal-entities/([\\w-]+)",
    "headers": {
      "X-B3-TraceId": {
        "matches": "[\\w-]+"
      },
      "X-B3-SpanId": {
        "matches": "[\\w-]+"
      },
      "X-TID": {
        "contains": "tenant1"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "id": "internal-id",
      "externalId": "external-id"
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
